{
  "name": "@betmate-ap/contracts",
  "version": "0.1.0",
  "private": false,
  "description": "GraphQL schema and generated TypeScript types for BetMate",
  "type": "module",
  "main": "generated/schema-types.ts",
  "types": "generated/schema-types.ts",
  "files": ["generated", "src/schema"],
  "scripts": {
    "codegen": "graphql-codegen --config codegen.ts",
    "build": "npm run codegen && tsc -p tsconfig.json"
  },
  "keywords": ["graphql", "schema", "types", "betmate"],
  "author": "betmate-ap",
  "license": "MIT",
  "dependencies": {
    "graphql": "^16.11.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-resolvers": "^4.5.1",
    "typescript": "^5.9.2"
  }
}
